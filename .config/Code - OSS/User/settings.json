{
    "files.autoSave": "onWindowChange",
    "editor.fontFamily": "'Fira Code', Consolas, 'Courier New', monospace, 'Font Awesome 5 Free'",
    "editor.fontLigatures": true,
    "editor.autoClosingBrackets": "beforeWhitespace",
    "editor.autoClosingQuotes": "beforeWhitespace",
    "editor.mouseWheelZoom": true,
    "editor.tabCompletion": "on",
    "workbench.iconTheme": "vscode-icons",
    "zenMode.hideLineNumbers": false,
    "extensions.closeExtensionDetailsOnViewChange": true,
    "terminal.explorerKind": "external",
    "terminal.integrated.confirmOnExit": true,
    "terminal.integrated.cursorBlinking": true,
    "remote.downloadExtensionsLocally": true,
    "sync.autoUpload": true,
    "sync.quietSync": true,
    "merge-conflict.autoNavigateNextConflict.enabled": true,
    "bracketPairColorizer.highlightActiveScope": true,
    "editor.matchBrackets": "never",
    "timeline.showView": true,
    //Extensions
    "code-runner.fileDirectoryAsCwd": true,
    "code-runner.saveFileBeforeRun": true,
    "code-runner.clearPreviousOutput": true,
    "code-runner.runInTerminal": true,
    "code-runner.executorMap": {
        "ahk": "autohotkey",
        "applescript": "osascript",
        "autoit": "autoit3",
        "bat": "cmd /c",
        "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "clojure": "lein exec",
        "coffeescript": "coffee",
        "cpp": "cd $dir && g++ $fileName -O2 -g -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "crystal": "crystal",
        "csharp": "scriptcs",
        "d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
        "dart": "dart",
        "fsharp": "fsi",
        "go": "go run",
        "groovy": "groovy",
        "haskell": "runhaskell",
        "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
        "html": "\"D:\\Program Files (x86)\\Microsoft\\Edge\\Application\\msedge.exe\"",
        "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
        "javascript": "node",
        "julia": "julia",
        "kit": "kitc --run",
        "lisp": "sbcl --script",
        "lua": "lua",
        "nim": "nim compile --verbosity:0 --hints:off --run",
        "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "ocaml": "ocaml",
        "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
        "perl": "perl",
        "perl6": "perl6",
        "php": "php",
        "powershell": "powershell -ExecutionPolicy ByPass -File",
        "python": "python -u",
        "r": "Rscript",
        "racket": "racket",
        "ruby": "ruby",
        "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
        "scala": "scala",
        "scheme": "csi -script",
        "shellscript": "bash",
        "swift": "swift",
        "typescript": "ts-node",
        "v": "v run",
        "vbscript": "cscript //Nologo"
    },
    "vim.ignorecase": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.leader": "<space>",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "j"
            ],
            "after": [
                "g",
                "j"
            ]
        },
        {
            "before": [
                "k"
            ],
            "after":[
                "g",
                "k"
            ]
        },
        {
            "before": [
                "<leader>",
                "m"
            ],
            "commands": [
                "bookmarks.toggle"
            ]
        },
        {
            "before": [
                "<leader>",
                "b"
            ],
            "commands": [
                "bookmarks.list"
            ]
        },
        {
            "before": [
                "Z",
                "Z"
            ],
            "commands": [
                ":wq"
            ]
        },
        {
            "before": [
                "<C-w>",
                "o"
            ],
            "commands": [
                "workbench.action.toggleEditorGroupLayout",
                "workbench.action.focusFirstEditorGroup"
            ]
        },
        {
            "before": [
                "<C-w>",
                "k"
            ],
            "commands": [
                "workbench.action.focusAboveGroup"
            ]
        },
        {
            "before": [
                "<C-w>",
                "j"
            ],
            "commands": [ "workbench.action.focusBelowGroup" ] },
                   {
            "before": [
                "<C-w>",
                "h"
            ],
            "commands": [
                "workbench.action.focusLeftGroup"
            ]
        },
        {
            "before": [
                "<C-w>",
                "l"
            ],
            "commands": [
                "workbench.action.focusRightGroup"
            ]
        },
        {
            "before": [
                "<leader>",
                "f"
            ],
            "after": [
                "z",
                "a"
            ]
        },
        {
            "before": [
                "<leader>",
                "h"
            ],
            "commands": [
                "psi-header.insertFileHeader"
            ]
        },
        {
            "before": [
                "<leader>",
                "n"
            ],
            "commands": [
                "code-runner.run",
                "workbench.action.terminal.toggleTerminal"
            ]
        },
        {
            "before": [
                "<leader>",
                "j"
            ],
            "commands": [
                "workbench.action.previousEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "k"
            ],
            "commands": [
                "workbench.action.nextEditor"
            ]
        }
    ],
    "background.customImages":["file:///C:/Users/pisceskkk/Pictures/3.jpg"],
    "background.style": {
        "content": "''",
        "pointer-events": "none",
        "position": "absolute",
        "z-index": "99999",
        "width": "100%",
        "height": "100%",
        "background-position": "center",
        "background-repeat": "no-repeat",
        "background-size": "100%,100%",
        "opacity": 0.1
    },
    "background.useDefault": false,
    "background.useFront": false,
    "window.zoomLevel": 0,
    "sync.gist": "5082baaee6bc934f1773582d275d0620",
    "latex-workshop.bibtex-format.tab": "4 spaces",
    "editor.renderWhitespace": "none",
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.insert": "#5f0000",
    "vim.statusBarColors.normal": "#005f5f",
    "vim.statusBarColors.replace": "#00000",
    "vim.statusBarColors.visual": "#5f00af",
    "vim.statusBarColors.visualline": "#8f00af",
    "vim.statusBarColors.visualblock": "#af00af",
    "vim.statusBarColors.searchinprogressmode": "#00aacc",
    "vim.statusBarColors.commandlineinprogress": "#007acc",
    "workbench.colorCustomizations": {
        "statusBar.background": "#007acc",
        "statusBar.noFolderBackground": "#007acc",
        "statusBar.debuggingBackground": "#007acc"
    },
    "C_Cpp.updateChannel": "Insiders",
    "sync.autoDownload": true,
    "psi-header.config.author": "pisceskkk",
    "psi-header.config.forceToTop": true,
    "psi-header.templates": [
        {
            "language": "*",
            "template": [
                "File: <<filename>>",
                "Goal: ",
                "Date: <<filecreated('YYYY/MM/DD h:mm)>>",
                "Author: <<author>>"
            ]
        }
    ],
    "psi-header.lang-config": [
        {
            "language": "markdown",
            "mapTo": "markdown",
            "begin": "<!--",
            "end": "-->",
            "prefix": "# ",
            "forceToTop": true,
        }
    ],
    "C_Cpp.default.intelliSenseMode": "gcc-x86",
    "cSpell.userWords": [
        "HYSBZ",
        "dfsn",
        "lson",
        "pisceskkk"
    ],
    "diffEditor.ignoreTrimWhitespace": true,
    "workbench.colorTheme": "Visual Studio Dark",
    "[html]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "markdown-pdf.headerTemplate": "<div style=\"font-size: 9px; margin-left: 1cm;\"> <span class='title'></span></div> <div style=\"font-size: 9px; margin-left: auto; margin-right: 1cm; \"><p>Chunshuo Qiu <span class='date'></span></p></div>",
}